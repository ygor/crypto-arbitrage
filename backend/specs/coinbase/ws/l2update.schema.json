{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.local/coinbase/ws/l2update.schema.json",
  "title": "Coinbase WebSocket L2 Update",
  "type": "object",
  "required": ["type", "product_id", "changes"],
  "properties": {
    "type": { "const": "l2update" },
    "product_id": { "type": "string" },
    "time": { "type": "string" },
    "changes": {
      "type": "array",
      "minItems": 1,
      "items": {
        "type": "array",
        "prefixItems": [
          { "type": "string" },
          { "type": "string" },
          { "type": "string" }
        ],
        "minItems": 3,
        "maxItems": 3
      }
    }
  },
  "additionalProperties": true
} 