{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://schemas.local/coinbase/ws/snapshot.schema.json",
  "title": "Coinbase WebSocket Snapshot",
  "type": "object",
  "required": ["type", "product_id", "bids", "asks"],
  "properties": {
    "type": { "const": "snapshot" },
    "product_id": { "type": "string" },
    "bids": {
      "type": "array",
      "items": {
        "type": "array",
        "prefixItems": [
          { "type": "string" },
          { "type": "string" }
        ],
        "items": { "type": "string" },
        "minItems": 2
      }
    },
    "asks": {
      "type": "array",
      "items": {
        "type": "array",
        "prefixItems": [
          { "type": "string" },
          { "type": "string" }
        ],
        "items": { "type": "string" },
        "minItems": 2
      }
    }
  },
  "additionalProperties": true
} 